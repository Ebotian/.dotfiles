[credential]
	credentialStore = gpg
	helper = /usr/local/bin/git-credential-manager
[credential "https://dev.azure.com"]
	useHttpPath = true
[user]
	name = Yibotian
	email = yiboxiaotian@nuaa.edu.cn
	signingkey = $HOME/.ssh/$HOST.pub
